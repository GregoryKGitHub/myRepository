import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int number = input.nextInt();
        int sum = 0;
        for (int i = 0; i < number; i++) {
            if (i % 3 == 0) {
                sum += i;
            }
            if (i % 5 == 0 && i % 3 != 0) {
                sum += i;
            }
        }
        System.out.println(sum);
    }
}
