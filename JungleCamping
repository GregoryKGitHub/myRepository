import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	Scanner input = new Scanner(System.in);
	String output = "";
	ArrayList<String> noises = new ArrayList<String>(Arrays.asList(input.nextLine().split(" ")));
        for (String s:
             noises) {
            switch (s)  {
                case "Grr":
                    output += "Lion ";
                    break;
                case "Rawr":
                    output += "Tiger ";
                    break;
                case "Ssss":
                    output += "Snake ";
                    break;
                case "Chirp":
                    output += "Bird ";
            }
        }
        System.out.println(output);
    }
}
