import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int houses = input.nextInt();
        if(200 % houses == 0)
        {
            System.out.println(200 / houses);
        }
        else
        {
            System.out.println(200 / houses + 1);
        }
    }
}
